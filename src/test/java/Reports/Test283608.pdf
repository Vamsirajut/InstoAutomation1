/* custom variables */

@keyframes spinner-pulse-dark {
  0% {
    background: rgba(22, 22, 23, 0.9);
  }

  50% {
    background: rgba(22, 22, 23, 0.6);
  }

  100% {
    background: rgba(22, 22, 23, 0.9);
  }
}

@keyframes spinner-pulse-light {
  0% {
    background: rgba(255, 255, 255, 0.9);
  }

  50% {
    background: rgba(255, 255, 255, 0.6);
  }

  100% {
    background: rgba(255, 255, 255, 0.9);
  }
}

.spinner {
  position: relative;
}

.spinner--overlay {
  z-index: 98;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #161617;
}

.spinner--overlay.spinner--transparent {
  background: rgba(22, 22, 23, 0.9);
  animation: spinner-pulse-dark 2s infinite linear;
}

.spinner--full-screen {
  height: 100vh;
  z-index: 99;
}

.spinner--invert.spinner--overlay {
  background: #fff;
}

.spinner--invert.spinner--transparent {
  animation-name: spinner-pulse-light;
}

.spinner--invert.spinner--mono .spinner__circle {
  border-top-color: rgba(57, 74, 88, 0.3);
